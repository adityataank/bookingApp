<h1>Users Registered</h1>
<table class="table table-striped">
    <tr>
        <th>User ID</th>
        <th>User E-Mail</th>
    </tr>
    <% @users.each do |user|%>
        <tr>
            <td>
                <%= user.id%>
            </td>
            <td>
                <%= user.email%>
            </td>
            <td>
                <%= link_to "Show" , admin_users_show_path(user)%>
            </td>
            <td>
                <%= link_to "Destroy", admin_destroy_user_path(user), method: :delete, data: { confirm: "You sure?" } %>
            </td>

            
        </tr>
    
    <% end %>
    

</table>